    "codemirror": "5.48.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "3.1.0",
    "del": "5.0.0",
    "eslint": "6.0.1",
    "eslint-config-google": "0.13.0",
    "exectimer": "2.2.1",
    "fetch-mock": "7.3.7",
    "file-loader": "4.1.0",
    "globby": "10.0.1",
    "google-closure-compiler": "20190709.0.0",
    "got": "9.6.0",
    "gulp": "4.0.2",
    "gulp-amphtml-validator": "1.0.2",
    "gulp-once": "2.0.3",
    "gulp-sass": "4.0.2",
    "gulp-strip-css-comments": "2.0.0",
    "handlebars": "4.1.2",
