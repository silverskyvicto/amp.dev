    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "octonode": "0.9.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "rcs-core": "2.6.0",
    "recursive-search": "1.0.1",
    "sass-loader": "7.1.0",
