    "write-yaml": "1.0.0"
  },
  "sideEffects": [
    "./playground/src/request-idle-callback/base.js",
    "./playground/src/service-worker/base.js",
    "./playground/src/event-listener-options/base.js",
    "*.css",
    "*.scss"
  ]
}
